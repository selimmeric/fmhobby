void LED_RGB(unsigned char iVal)
{

unsigned char tPort;

tPort = LATC & (~(0x07<<2));
tPort = tPort | ((~iVal & 0x07)<<2);
LATC = tPort;

}

void Buzzer(unsigned char iVal)
{
unsigned char tPort;

tPort = LATC & (~(1<<5));
tPort = tPort | ((iVal & 0x01)<<5);
LATC = tPort;

}


void UVL(unsigned char iVal)
{
unsigned char tPort;

tPort = LATC & (~(1<<0));
tPort = tPort | ((iVal & 0x01)<<0);
LATC = tPort;

}




unsigned char FS_RD(unsigned char iFS)
{
static unsigned char pFS=0;
unsigned int tFS=0;

tFS = iFS;

if (pFS > iFS)
tFS += 0x100;

tFS -= pFS;

pFS = iFS;

return (tFS & 0xff);

}


unsigned long RFID_RD(void)
{
unsigned int i=0;
unsigned long tRFID=0;;


LATA &= ~(1<<4);
LATC &= ~(1<<1);



LATA |= (1<<4);
for (i=0; i<2; i++)
{
LATC |= ((1<<1));
LATC &= ~((1<<1));
}
LATA &= ~(1<<4);


for (i=0; i<16; i++)
{
LATC |= ((1<<1));
LATC &= ~((1<<1));
tRFID = tRFID << 1;
if ((PORTA & (1<<5)))
tRFID +=1;
}

return (tRFID);
}
